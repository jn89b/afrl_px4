choice
	prompt "Boot Flash"
	default NXP_FMURT1062_V3_QSPI_FLASH

config NXP_FMURT1062_V3_HYPER_FLASH
	bool "HYPER Flash"

config NXP_FMURT1062_V3_QSPI_FLASH
	bool "QSPI Flash"

endchoice # Boot Flash
